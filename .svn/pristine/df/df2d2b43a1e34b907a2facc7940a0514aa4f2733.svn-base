* {
    margin: 0px;
    padding: 0px;
}

body {
    /* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#eceeef+0,f6f6f6+52,eceeef+100 */

}

header {
    min-height: 200px;

}

header div.box-top {
    min-height: 120px;
    border: 1px solid #c4c4c3;
    background: rgb(236, 238, 239); /* Old browsers */
    background: -moz-radial-gradient(center, ellipse cover, rgba(236, 238, 239, 1) 0%, rgba(246, 246, 246, 1) 52%, rgba(236, 238, 239, 1) 100%); /* FF3.6-15 */
    background: -webkit-radial-gradient(center, ellipse cover, rgba(236, 238, 239, 1) 0%, rgba(246, 246, 246, 1) 52%, rgba(236, 238, 239, 1) 100%); /* Chrome10-25,Safari5.1-6 */
    background: radial-gradient(ellipse at center, rgba(236, 238, 239, 1) 0%, rgba(246, 246, 246, 1) 52%, rgba(236, 238, 239, 1) 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#eceeef', endColorstr='#eceeef', GradientType=1); /* IE6-9 fallback on horizontal gradient */

}

header div.box-top div.box-header {
    border-bottom: 1px solid #c4c4c3;
    height: 20px;
}

div.contenu {
    padding: 30px;
}