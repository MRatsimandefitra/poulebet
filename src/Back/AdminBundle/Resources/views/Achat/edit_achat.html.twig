{% extends 'BackAdminBundle::layout_match.html.twig' %}
{% block title %} {{ parent() }} {% endblock %}
{% block ariane %} {{ parent() }} {% endblock %}

{% block page %}
    <div class="col-lg-12">

        <div class="col-lg-12">
            {{ form_start(form) }}
            <div class="input">
                {{ 'Image' }}
                {{ form_widget(form.imageOeuf) }}
                {{ form_errors(form.imageOeuf) }}
            </div>
            <div class="input">
                {{ 'Tarifs Euro' }}
                {{ form_widget(form.tarifOeufs) }}
                {{ form_errors(form.tarifOeufs) }}
            </div>
            <div class="input">
                {{ 'Image' }}
                {{ form_widget(form.tarifEuro) }}
                {{ form_errors(form.tarifEuro) }}
            </div>
            <div class="input">
                {{ 'Image' }}
                {{ form_widget(form.tarifOeufs) }}
                {{ form_errors(form.tarifOeufs) }}
            </div>
            <div class="input">
                <input type="submit" class="btn btn-primary" value="{{ 'Ajouter'|trans }}"/>
            </div>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}
