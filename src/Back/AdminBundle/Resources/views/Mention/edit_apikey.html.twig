{% extends 'BackAdminBundle::layout.html.twig' %}
{% block title %} {{ parent() }} {% endblock %}
{% block ariane %} {{ parent() }} {% endblock %}

{% block page %}

    <div class="sous-menu-page">
        <div class="col-lg-12">
            <div class="col-lg-12">
                {{ 'Listes des droits'|trans }}
                {% include 'BackAdminBundle:Mention:sous_menu.html.twig' %}
            </div>

        </div>
    </div>
    <div class="box-response">
        {{ form_start(form) }}
            <table class="table">
                <tr>
                    <td>
                        {{ 'ApiKey' }}
                    </td>
                    <td>
                        {{ form_widget(form.apikey) }}
                    </td>
                </tr>
                <tr>
                    <td>
                        {{ 'Date d√©but valide'|trans }}
                    </td>
                    <td>
                        {{ form_widget(form.dateDebutValide) }}
                    </td>
                </tr>
                <tr>
                    <td>
                        {{ 'Date finale valide' }}
                    </td>
                    <td>
                        {{ form_widget(form.dateFinaleValide) }}
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                        <input type="submit" value="{{ 'Enregister' }}"/>
                    </td>
                </tr>
            </table>

        {{ form_end(form) }}
   </div>
{% endblock %}